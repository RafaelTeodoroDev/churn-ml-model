{
  "name": "churn-prediction-api-2025",
  "version": 2,
  "builds": [
    {
      "src": "vercel_api.py",
      "use": "@vercel/python"
    },
    {
      "src": "frontend/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/vercel_api.py"
    },
    {
      "src": "/health",
      "dest": "/vercel_api.py"
    },
    {
      "src": "/docs",
      "dest": "/vercel_api.py"
    },
    {
      "src": "/model/info",
      "dest": "/vercel_api.py"
    },
    {
      "src": "/features/importance",
      "dest": "/vercel_api.py"
    },
    {
      "src": "/sample/customer",
      "dest": "/vercel_api.py"
    },
    {
      "src": "/predict",
      "dest": "/vercel_api.py"
    },
    {
      "src": "/predict/batch",
      "dest": "/vercel_api.py"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/$1"
    }
  ]
}
